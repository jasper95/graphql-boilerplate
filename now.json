{
  "name": "graph-spike",
  "version": 2,
  "builds": [
    { "src": "public/**/*", "use": "@now/static" },
    { "src": "build/**/*", "use": "@now/static" },
    { "src": "dist/index.js", "use": "@now/node" }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/build/$1" },
    { "src": "/service-worker.js", "dest": "/build/service-worker.js" },
    { "src": "/(.*)", "dest": "/dist/index.js" }
  ]
}